@model SignInDto

<div class="row">
    <div class="col-12">
        <form asp-controller="Auth" asp-action="Signin" method="post">
            <div class="mb-3">
                <label asp-for="Email" class="form-label">Email address</label>
                <input class="form-control" asp-for="Email" aria-describedby="emailHelp">
                <span asp-validation-for="Email"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Password" class="form-label">Password</label>
                <input asp-for="Password" class="form-control" >
                <span asp-validation-for="Password"></span>
            </div>
            <div class="mb-3 form-check">
                <input asp-for="RememberMe" class="form-check-input" id="exampleCheck1">
                <label asp-for="RememberMe" for="exampleCheck1">Remember Me</label>
            </div>
            <button type="submit" class="btn btn-success">SignIn</button>
        </form>
    </div>
</div>
